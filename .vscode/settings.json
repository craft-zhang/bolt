{
    "git.ignoreLimitWarning": true,
    "editor.formatOnSave": true,

    "psi-header.config": {
        "forceToTop": true,
        "blankLinesAfter": 2,
        "license": "Custom"
    },
    "psi-header.changes-tracking": {
        "isActive": true,
        "modAuthor": "Modified By: ",
        "modDate": "Last Modified: ",
        "modDateFormat": "date",
        "include": [],
        "exclude": [
            "markdown",
            "json"
        ],
        "includeGlob": [],
        "autoHeader": "manualSave"
    },
    "psi-header.license-text": [
        "This File Is Part Of Cloudwalk Feature Clustering Similarity Calculs."
    ],
    "psi-header.variables": [
        [
            "company",
            "CloudWalk Technology Co., Ltd.."
        ],
        [
            "author",
            "ZhangDanfeng"
        ],
        [
            "authoremail",
            "zhangdanfeng@cloudwalk.com"
        ]
    ],
    "psi-header.lang-config": [
        {
            "language": "lua",
            "begin": "--[[",
            "prefix": "--",
            "end": "--]]",
            "blankLinesAfter": 0
        },
        {
            "language": "python",
            "begin": "###",
            "prefix": "# ",
            "end": "###",
            "blankLinesAfter": 0,
            "beforeHeader": [
                "#!/usr/bin/env python3",
                "# -*- coding:utf-8 -*-"
            ]
        },
        {
            "language": "shellscript",
            "begin": "#---------------------------------------------------------------------------",
            "prefix": "# ",
            "end": "#---------------------------------------------------------------------------",
            "blankLinesAfter": 0,
            "beforeHeader": [
                "#!/bin/bash"
            ]
        },
        {
            "language": "javascript",
            "begin": "/**",
            "prefix": " * ",
            "end": " */",
            "blankLinesAfter": 2,
            "forceToTop": false
        },
        {
            "language": "typescript",
            "mapTo": "javascript"
        }
    ],
    "psi-header.templates": [
        {
            "language": "*",
            "template": [
                "File: <<filename>>",
                "Created Date: <<filecreated('YYYY-MM-DD')>>",
                "Author: <<author>>",
                "Contact: <<<authoremail>>>",
                "",
                "Last Modified: <<dateformat('dddd MMMM Do YYYY h:mm:ss a')>>",
                "",
                "Copyright (c) <<year>> <<company>>",
                "<<licensetext>>",
                "-----",
                "HISTORY:",
                "Date      By      Comments",
                "-------------------------------------------------------------------------"
            ],
            "changeLogCaption": "HISTORY:",
            "changeLogHeaderLineCount": 2,
            "changeLogEntryTemplate": [
                "",
                "<<dateformat(DD-MM-YYYY)>>\t<<initials>>\t"
            ]
        },
        {
            "language": "javascript",
            "template": [
                "File: <<filepath>>",
                "Project: <<projectpath>>",
                "Created Date: <<filecreated('YYYY-MM-DD  h:mm:ss')>>",
                "Author: <<author>>",
                "-----",
                "Last Modified: ",
                "Modified By: ",
                "-----",
                "Copyright (c) <<year>> <<company>>",
                "------------------------------------",
                "Javascript will save your soul!"
            ]
        },
        {
            "language": "typescript",
            "mapTo": "javascript"
        }
    ],

    "C_Cpp.errorSquiggles": "Disabled",
    "files.associations": {
        "*.S": "gas",
        "chrono": "cpp",
        "algorithm": "cpp",
        "array": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "vector": "cpp",
        "string_view": "cpp",
        "initializer_list": "cpp",
        "random": "cpp",
        "bitset": "cpp",
        "atomic": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "codecvt": "cpp",
        "condition_variable": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "forward_list": "cpp",
        "exception": "cpp",
        "functional": "cpp",
        "optional": "cpp",
        "ratio": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "fstream": "cpp",
        "iomanip": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "numeric": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "thread": "cpp",
        "cinttypes": "cpp",
        "regex": "cpp",
        "utility": "cpp",
        "typeinfo": "cpp",
        "iterator": "cpp",
        "map": "cpp",
        "memory_resource": "cpp",
        "set": "cpp",
        "shared_mutex": "cpp",
        "bit": "cpp",
        "asmmacro.h": "c",
        "sgemmkernelcommon.h": "c",
        "*.s": "cpp"
    },
    "editor.tabSize": 2,
    "editor.renderWhitespace": "all",
    "C_Cpp.formatting": "Default",
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "C_Cpp.clang_format_style": "Google",
    "C_Cpp.default.cppStandard": "gnu++17",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.defines": ["-D__aarch64__"],
}